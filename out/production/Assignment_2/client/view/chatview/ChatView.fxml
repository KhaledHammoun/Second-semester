<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.scene.control.Button?>
<?import javafx.scene.control.TextArea?>
<?import javafx.scene.layout.Pane?>

<Pane prefHeight="400.0" prefWidth="360.0" xmlns="http://javafx.com/javafx/11.0.1" xmlns:fx="http://javafx.com/fxml/1" fx:controller="client.view.chatview.ChatViewController">
   <children>
      <Button layoutX="318.0" layoutY="14.0" mnemonicParsing="false" onAction="#onExitButton" text="Exit" />
      <Button layoutX="298.0" layoutY="365.0" mnemonicParsing="false" onAction="#onFriendsButton" text="Friends" />
      <TextArea fx:id="receiveMessageTextArea" editable="false" layoutX="14.0" layoutY="14.0" prefHeight="306.0" prefWidth="260.0" wrapText="true" />
      <TextArea fx:id="messageTextArea" layoutX="14.0" layoutY="337.0" onKeyPressed="#onEnter" prefHeight="56.0" prefWidth="220.0" wrapText="true" />
      <Button layoutX="234.0" layoutY="337.0" mnemonicParsing="false" onAction="#onSendMessageButton" prefHeight="56.0" prefWidth="41.0" text="send" />
   </children>
</Pane>
