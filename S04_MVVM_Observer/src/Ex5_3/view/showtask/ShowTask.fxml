<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.geometry.Insets?>
<?import javafx.scene.control.Button?>
<?import javafx.scene.control.TableColumn?>
<?import javafx.scene.control.TableView?>
<?import javafx.scene.layout.HBox?>
<?import javafx.scene.layout.VBox?>

<VBox prefHeight="387.0" prefWidth="600.0" xmlns="http://javafx.com/javafx/11.0.1" xmlns:fx="http://javafx.com/fxml/1" fx:controller="Ex5_3.view.showtask.ShowTaskController">
   <children>
      <TableView fx:id="tasksTable" prefHeight="347.0" prefWidth="600.0">
        <columns>
          <TableColumn fx:id="creatorColumn" prefWidth="117.99999749660492" text="Creator" />
          <TableColumn fx:id="descriptionColumn" prefWidth="346.66661834716797" text="Description" />
            <TableColumn fx:id="dateColumn" prefWidth="135.33334350585938" text="Date" />
        </columns>
      </TableView>
      <HBox prefHeight="23.0" prefWidth="590.0" spacing="10.0">
         <children>
            <Button mnemonicParsing="false" onAction="#onAddButton" text="Add" />
            <Button mnemonicParsing="false" onAction="#onNextTaskButton" text="Next task" />
         </children>
         <VBox.margin>
            <Insets left="5.0" top="10.0" />
         </VBox.margin>
      </HBox>
   </children>
</VBox>
